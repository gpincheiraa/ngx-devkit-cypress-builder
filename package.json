  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^12.7.12",
    "cpr": "^3.0.1",
    "jest": "^24.7.1",
    "jest-junit": "^8.0.0",
    "ts-jest": "^24.0.2",
    "typescript": ">=3.4.0 <3.6.0"
